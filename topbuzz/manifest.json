{
    "name": "TopBuzz",
    "version": "0.6",
    "description": "Most pop videos from Musical.ly, TopBuzz...",
    "permissions": [
      "activeTab", 
      "storage", 
      "tabs", 
      "\u003Call_urls\u003E"
      ],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "icons": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "manifest_version": 2
  }
